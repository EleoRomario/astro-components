{
  "extends": [
    "plugin:astro/recommended",
    "eslint:recommended"
  ],
  "overrides": [
      {
        "files": ["*.astro"],
        "parser": "astro-eslint-parser",
        "parserOptions": {
          "parser": "@typescript-eslint/parser",
          "extraFileExtensions": [".astro"]
        },
        "rules": {
          // "astro/no-set-html-directive": "error",
          "no-unused-vars": "warn"
        }
      }
  ]
}
